    "preds = []\n",
    "fired = []\n",
    "\n",
    "for _, row in logs_df.iterrows():\n",
    "    label = 0\n",
    "    fr = \"\"\n",
    "    for r in rules:\n",
    "        cond = r.split(\"THEN\")[0].replace(\"IF\", \"\").strip()\n",
    "        # replace field names with row values\n",
    "        for c in row.index:\n",
    "            cond = cond.replace(c, str(row[c]))\n",
    "        try:\n",
    "            if eval(cond):\n",
    "                label = 1\n",
    "                fr = r\n",
    "                break\n",
    "        except:\n",
    "            continue\n",
    "    preds.append(label)\n",
    "    fired.append(fr)\n",
    "\n",
    "logs_df[\"predicted_label\"] = preds\n",
    "logs_df[\"fired_rule\"] = fired\n",
    "\n",
    "# Print top 20 only\n",
    "print(\"\\nTop 20 logs with predictions:\")\n",
    "print(logs_df.head(20)[[\"session_id\", \"predicted_label\", \"fired_rule\"]])"
